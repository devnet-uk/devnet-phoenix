{
  "name": "@phoenix/auth",
  "version": "0.0.1",
  "description": "Better-Auth configuration and authentication utilities for Phoenix",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@phoenix/contracts": "workspace:*",
    "better-auth": "^1.3.7"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": ["authentication", "better-auth", "security"],
  "author": "DevNet Engineering <engineering@devnet.uk>",
  "license": "MIT",
  "publishConfig": {
    "access": "restricted"
  }
}